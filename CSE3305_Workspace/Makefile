.PHONY: clean All

All:
	@echo "----------Building project:[ Thompson_CS3305_Lab2 - Debug ]----------"
	@cd "Thompson_CS3305_Lab02" && "$(MAKE)" -f  "Thompson_CS3305_Lab2.mk"
clean:
	@echo "----------Cleaning project:[ Thompson_CS3305_Lab2 - Debug ]----------"
	@cd "Thompson_CS3305_Lab02" && "$(MAKE)" -f  "Thompson_CS3305_Lab2.mk" clean
