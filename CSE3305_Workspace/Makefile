.PHONY: clean All

All:
	@echo "----------Building project:[ Thompson_CS3305_Lab10 - Debug ]----------"
	@cd "Thompson_CS3305_Lab10" && "$(MAKE)" -f  "Thompson_CS3305_Lab10.mk"
clean:
	@echo "----------Cleaning project:[ Thompson_CS3305_Lab10 - Debug ]----------"
	@cd "Thompson_CS3305_Lab10" && "$(MAKE)" -f  "Thompson_CS3305_Lab10.mk" clean
